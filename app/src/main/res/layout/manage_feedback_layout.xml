<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="120dp"
    android:layout_marginBottom="4dp"
    android:background="#F1F7F7"
    android:orientation="horizontal">


    <TextView
        android:id="@+id/manageFeedbackName"
        android:layout_width="wrap_content"
        android:layout_height="54dp"
        android:layout_x="113dp"
        android:layout_y="32dp"
        android:gravity="center"
        android:text="TextView"
        android:textColor="#6F0729"
        android:textSize="20dp"
        android:textStyle="bold|italic" />

    <TextView
        android:id="@+id/manageFeedbackTitle"
        android:layout_width="179dp"
        android:layout_height="29dp"
        android:layout_x="214dp"
        android:layout_y="22dp"
        android:gravity="center"
        android:text="TextView"
        android:textColor="#000"
        android:textSize="16dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/manageFeedbackContact"
        android:layout_width="179dp"
        android:layout_height="32dp"
        android:layout_x="213dp"
        android:layout_y="60dp"
        android:gravity="center"
        android:text="TextView"
        android:textSize="12dp" />

    <ImageView
        android:id="@+id/imageViewFeedBack"
        android:layout_width="83dp"
        android:layout_height="78dp"
        android:layout_x="0dp"
        android:layout_y="19dp"
        android:paddingTop="5dp"
        android:paddingBottom="5dp"
        tools:srcCompat="@tools:sample/avatars" />
</AbsoluteLayout>
